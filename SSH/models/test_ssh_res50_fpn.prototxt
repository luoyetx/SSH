    pad: 0
    kernel_size: 1
    weight_filler { type: "gaussian" std: 0.01 }
    bias_filler { type: "constant" value: 0 }
  }
}

### res4f_feat

# up scale
layer {
  name: "res5c_feat_up_scale"
  type: "Deconvolution"
  bottom: "res5c_feat"
  top: "res5c_feat_up_scale"
  convolution_param {
    kernel_size: 4
    stride: 2
    num_output: 256
    group: 256
    pad: 1
    weight_filler: { type: "bilinear" }
    bias_term: false
  }
  param { lr_mult: 0 decay_mult: 0 }
}

# reduce dim
layer {
  name: "res4f_feat_reduce_dim"
  type: "Convolution"
  bottom: "res4f"
  top: "res4f_feat_reduce_dim"
  param {
    lr_mult: 1
  }
  param {
    lr_mult: 2
  }
  convolution_param {
    num_output: 256
    pad: 0
    kernel_size: 1
    weight_filler { type: "gaussian" std: 0.01 }
    bias_filler { type: "constant" value: 0 }
  }
}

# crop
layer {
  name: "res4f_feat_crop"
  type: "Crop"
  bottom: "res5c_feat_up_scale"
  bottom: "res4f_feat_reduce_dim"
  top: "res5c_feat_up_scale_crop"
  crop_param {
    axis: 2
    offset: 0
  }
}

# add
layer {
  name: "res4f_feat_add"
  type: "Eltwise"
  bottom: "res5c_feat_up_scale_crop"
  bottom: "res4f_feat_reduce_dim"
  top: "res4f_feat_add"
  eltwise_param {
    operation: SUM
  }
}

# fuse
layer {
  name: "res4f_feat"
  type: "Convolution"
  bottom: "res4f_feat_add"
  top: "res4f_feat"
  param {
    lr_mult: 1
